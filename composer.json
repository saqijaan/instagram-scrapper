{
  "name": "saqijaan/instagram-scraper",
  "description": "Instagram Scraper  based on raiym/instagram-php-scraper . Get account information, photos and videos without any authorization. for some blocked endpoints you can just place a cookie file and can retrive data bypassing instagram blockage",
  "keywords": [
    "instagram",
    "scrapper",
    "instagram stories",
    "instagram dp"
  ],
  "type": "library",
  "homepage": "https://github.com/saqijaan/instagram-scrapper",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Saqib Ali",
      "email": "saqiba874@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "mashape/unirest-php": "3.0.*",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/simple-cache": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*",
    "phpfastcache/phpfastcache": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "InstagramScraper\\": "src/InstagramScraper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InstagramScraper\\Tests\\": "tests/"
    }
  },
  "support": {
    "email": "saqiba874@gmail.com"
  }
}
